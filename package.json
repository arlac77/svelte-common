{
  "name": "svelte-common",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "svelte": "./src/index.svelte",
  "description": "common components and utils used in svelte apps",
  "keywords": [
    "component",
    "svelte",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "start": "rollup -c example/rollup.config.mjs -w",
    "test": "testcafe $BROWSER:headless tests/*-test.js -s test-results --app-init-delay 1000 --app \"rollup -c example/rollup.config.mjs -w\"",
    "docs": "documentation readme ./src/index.mjs --section=API",
    "lint": "documentation lint ./src/index.mjs"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "documentation": "^13.0.2",
    "rollup": "^2.26.6",
    "rollup-plugin-dev": "^1.1.2",
    "rollup-plugin-svelte": "^6.0.0",
    "semantic-release": "^17.1.1",
    "svelte": "^3.24.1",
    "testcafe": "^1.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/svelte-common.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/svelte-common/issues"
  },
  "homepage": "https://github.com/arlac77/svelte-common#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-svelte-component"
    ]
  }
}
