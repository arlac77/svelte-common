{
  "name": "svelte-common",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "svelte": "./src/index.svelte",
  "description": "common components and utils used in svelte apps",
  "keywords": [
    "component",
    "svelte",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "start": "rollup -c tests/app/rollup.config.mjs -w",
    "test": "npm run test:cafe",
    "test:cafe": "testcafe $BROWSER:headless tests/cafe/*.js -s build/test --app-init-delay 1000 --app \"rollup -c tests/app/rollup.config.mjs -w\"",
    "docs": "documentation readme ./src/index.mjs --section=API && documentation readme ./src/index.svelte --section=API",
    "lint": "documentation lint ./src/index.mjs && npm run lint:docs",
    "lint:docs": "documentation lint ./src/index.mjs && documentation lint ./src/index.svelte"
  },
  "dependencies": {
    "svelte-entitlement": "^1.1.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-virtual": "^2.0.3",
    "documentation": "github:arlac77/documentation",
    "mf-styling": "arlac77/mf-styling",
    "postcss-import": "12.0.1",
    "rollup": "^2.35.1",
    "rollup-plugin-dev": "^1.1.3",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "semantic-release": "^17.3.0",
    "svelte": "^3.31.0",
    "testcafe": "^1.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/svelte-common.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/svelte-common/issues"
  },
  "homepage": "https://github.com/arlac77/svelte-common#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-svelte-component"
    ]
  }
}
