{
  "name": "svelte-common",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "svelte": "./src/index.svelte",
  "description": "common components and utils used in svelte apps",
  "keywords": [
    "component",
    "svelte",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "start": "vite dev",
    "test": "npm run test:ava && npm run test:cafe",
    "test:cafe": "testcafe $BROWSER:headless tests/cafe/*.js -s build/test --page-request-timeout 9000 --app-init-delay 3000 --app \"vite dev\"",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "documentation lint ./src/index.mjs && npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "dependencies": {
    "svelte-command": "^1.1.12",
    "svelte-entitlement": "^1.2.22"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
    "ava": "^4.3.0",
    "documentation": "^13.2.5",
    "mf-styling": "^1.2.22",
    "semantic-release": "^19.0.3",
    "svelte": "^3.48.0",
    "testcafe": "^1.19.0",
    "vite": "^2.9.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/svelte-common.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/svelte-common/issues"
  },
  "homepage": "https://github.com/arlac77/svelte-common#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-ava",
      "arlac77/template-cloudflare",
      "arlac77/template-svelte-component#next"
    ]
  }
}
