{
  "name": "svelte-common",
  "version": "0.0.0-semantic-release",
  "module": "src/index.mjs",
  "svelte": "src/index.svelte",
  "description": "common components utils used in svelte apps",
  "keywords": [
    "svelte"
  ],
  "scripts": {
    "docs": "documentation readme src/index.mjs --section=API",
    "lint": "documentation lint src/index.mjs",
    "start": "rollup -c example/rollup.config.js -w",
    "test": "testcafe $BROWSER:headless tests/*-test.js -s test-results --app-init-delay 500 --app \"rollup -c example/rollup.config.js -w\""
  },
  "devDependencies": {
    "documentation": "^12.1.2",
    "rollup": "^1.26.0",
    "rollup-plugin-dev": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "semantic-release": "^15.13.27",
    "svelte": "^3.12.1",
    "testcafe": "^1.6.1"
  },
  "engines": {
    "node": ">=12.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/svelte-common.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/svelte-common/issues"
  },
  "homepage": "https://github.com/arlac77/svelte-common#readme",
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-svelte.git"
    }
  }
}
